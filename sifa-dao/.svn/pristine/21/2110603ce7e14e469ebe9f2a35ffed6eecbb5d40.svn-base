  <select id="selectPermissionIdListByRoleId" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT
            e.PERMISSION_ID AS id
        FROM
            SYS_ROLE r
        LEFT JOIN SYS_ROLE_PERMISSION e ON r.id = e.role_id
        WHERE
            r.id = #{id}
    </select>

    <select id="selectPermissionListByRoleIdList" resultType="com.rowell.sifa.pojo.sys.SysPermisson"
            parameterType="java.lang.String">
        SELECT
            DISTINCT s.id AS id,
            s.NAME AS name,
            s.url AS url,
            s.description AS description,
            s.icon AS icon,
            s.PARENT_ID AS parentId,
            s.per_Sort AS perSort,
            s.open_mode AS openMode,
            s.opened as opened,
            s.type
        
        FROM
            SYS_ROLE r
        LEFT JOIN SYS_ROLE_PERMISSION e ON r.id = e.role_id
        LEFT JOIN SYS_PERMISSON s ON e.PERMISSION_ID = s.id
        WHERE
            r.id in  
            <foreach item="item" index="index" collection="list" open="(" separator="," close=")">  
                #{item}
            </foreach>
        AND s.type = 0
        ORDER BY PER_SORT
    </select>
    
     <select id="selectPermissionMenuListByRoleId" resultType="com.rowell.sifa.pojo.sys.SysPermisson"
            parameterType="java.lang.String">
