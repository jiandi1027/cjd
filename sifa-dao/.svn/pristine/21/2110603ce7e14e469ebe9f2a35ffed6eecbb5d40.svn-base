            s.url AS url,
            s.description AS description,
            s.icon AS icon,
            s.PARENT_ID AS parentId,
            s.per_Sort AS perSort,
            s.open_mode AS openMode,
            s.opened as opened,
            s.type
        
        FROM
            SYS_ROLE r
        LEFT JOIN SYS_ROLE_PERMISSION e ON r.id = e.role_id
        LEFT JOIN SYS_PERMISSON s ON e.PERMISSION_ID = s.id
        WHERE
            r.id=#{id}
        AND s.type = '0'
        ORDER BY PER_SORT
    </select>

    <select id="selectPermissionListByRoleId" resultType="java.util.HashMap" parameterType="java.lang.String">
        SELECT
            e.id AS id,
            s.url AS url
        FROM
            SYS_ROLE r
        LEFT JOIN SYS_ROLE_PERMISSION e ON r.id = e.role_id
        LEFT JOIN SYS_PERMISSON s ON e.PERMISSION_ID = s.id
        WHERE
            s.del_flag=0 and r.id = #{id}
             ORDER BY PER_SORT
    </select>
