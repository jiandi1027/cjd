package com.rowell.sifa.pojo.sys;

import java.io.Serializable;

import com.rowell.sifa.pojo.base.DataEntity;

/**
 *   每年工作日节假日表
* @Author marcle
* @version 1.0 2017-03-16
 */
public class Holiday extends DataEntity<Holiday> implements Serializable {
  

      
	    /**  
	    * @Fields field:field:{todo}(用一句话描述这个变量表示什么)  
	    */  
	    
	private static final long serialVersionUID = -7031936918380466320L;

	/**年份 */
    private Integer year;

    /**月份 */
    private Integer month;

    /**日 */
    private Integer day;

    /**数据字典当天日期类型 */
    private String dicDayTypeKey;

  

   

    public Integer getYear() {
        return year;
    }

    public void setYear(Integer year) {
        this.year = year;
    }

    public Integer getMonth() {
        return month;
    }

    public void setMonth(Integer month) {
        this.month = month;
    }

    public Integer getDay() {
        return day;
    }

    public void setDay(Integer day) {
        this.day = day;
    }

    public String getDicDayTypeKey() {
        return dicDayTypeKey;
    }

    public void setDicDayTypeKey(String dicDayTypeKey) {
        this.dicDayTypeKey = dicDayTypeKey == null ? null : dicDayTypeKey.trim();
    }

}