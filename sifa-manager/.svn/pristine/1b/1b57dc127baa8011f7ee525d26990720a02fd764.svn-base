  <select id="selectAll" parameterType="com.rowell.sifa.pojo.dailymgr.SpecialArea" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />,t2.name as culprit_name,t3.name as group_name
    from SF_SPECIAL_AREA t left join SF_CULPRIT t2 on (t.CULPRIT_ID=t2.id) left join sys_group t3 on (t2.group_id=t3.id)
