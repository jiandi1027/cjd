  <sql id="findAllWhere">
  	<if test="culprit!=null and culprit.name != null and culprit.name!=''">
      AND t2.NAME like '%'|| #{culprit.name,jdbcType=VARCHAR}||'%'
    </if>
     <if test="groupId != null and groupId!=''">
     	 <![CDATA[  
     	 AND  exists ( select t4.id from (select * from sys_group  start with id= #{groupId,jdbcType=VARCHAR} connect by prior id = parent_id) t4 where t2.group_id=t4.id)
       ]]>  
    </if>
    <if test="culpritId != null and culpritId !=''">
      AND t.CULPRIT_ID=#{culpritId,jdbcType=VARCHAR}
     </if>
      <if test="dicCheckTypeId != null and dicCheckTypeId !=''">
       AND t.DIC_CHECK_TYPE_ID=#{dicCheckTypeId,jdbcType=VARCHAR}
      </if>
      <if test="area != null and area !=''">
       AND t.AREA= #{area,jdbcType=VARCHAR}
      </if>
      <if test="applyDate != null and applyDate !=''">
       AND t.APPLY_DATE= #{applyDate,jdbcType=TIMESTAMP}
      </if>
      <if test="reason != null and reason !=''">
       AND t.REASON=#{reason,jdbcType=VARCHAR}
      </if>
      <if test="startDate != null and startDate !=''">
       AND t.START_DATE=#{startDate,jdbcType=TIMESTAMP} 
      </if>
      <if test="endDate != null and endDate !=''">
        AND t.END_DATE=#{endDate,jdbcType=TIMESTAMP}
      </if>
      <if test="note != null and note !=''">
        AND t.NOTE=#{note,jdbcType=VARCHAR}
      </if>
      <if test="dicMonitorGradeKey != null and dicMonitorGradeKey !=''">
        AND t.DIC_MONITOR_GRADE_KEY=#{dicMonitorGradeKey,jdbcType=VARCHAR}
      </if>
      <if test="created != null and created !=''">
      	AND t.CREATED=#{created,jdbcType=TIMESTAMP}
      </if>
      <if test="searchStartDate != null">
	    <![CDATA[ 
	      AND t.APPLY_DATE >= #{searchStartDate,jdbcType=TIMESTAMP}
	       ]]> 
	  </if>
     <if test="searchEndDate != null">
    	 <![CDATA[  
      		AND t.APPLY_DATE <= #{searchEndDate,jdbcType=TIMESTAMP}
       ]]>  
    </if>
      <if test="creater != null and creater !=''">
      AND  t.CREATER=#{creater,jdbcType=VARCHAR}
      </if>
      <if test="updated != null and updated !=''">
      AND  t.UPDATED=#{updated,jdbcType=TIMESTAMP}
      </if>
      <if test="updater != null and updater !=''">
      AND  t.UPDATER=#{updater,jdbcType=VARCHAR}
      </if>
      <if test="delFlag != null and delFlag !=''">
      AND  t.DEL_FLAG=#{delFlag,jdbcType=DECIMAL}
      </if>
      <if test="processInstanceId != null and processInstanceId !=''">
