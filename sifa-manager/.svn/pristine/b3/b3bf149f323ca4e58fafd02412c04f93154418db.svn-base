			parent_ids = #{parentIds}
		WHERE id = #{id}
	</update>
  
   <select id="selectPermissionMenusByUserId" resultMap="BaseResultMap" parameterType="java.lang.String">
   	    SELECT
          <include refid="Base_Column_List" />
        from  sys_permisson t1            
        where 
        t1.del_flag=0 and 
	       exists(	       
	        select t2.permission_id  from  sys_role_permission t2 
	        left join sys_role t3 on t2.role_id = t3.id
	        left join sys_account t4 on (t3.id=t4.role_id)
	        where
	           t1.id = t2.permission_id  and  t4.id = #{userId}  and  t1.type='0'    
	       )    
        order by t1.per_sort asc  
    </select>
    
     <select id="selectPermissionOperationByUserId" resultType="java.lang.String" parameterType="java.lang.String">
   	    SELECT  t1.url  from  sys_permisson t1            
        where 
         t1.del_flag=0 and 
	       exists(	       
	        select t2.permission_id  from  sys_role_permission t2 
	        left join sys_role t3 on t2.role_id = t3.id
	        left join sys_account t4 on (t3.id=t4.role_id)
	        where
	           t1.id = t2.permission_id  and  t4.id = #{userId}  and  t1.type='1'    
	       )    
    </select>
    
