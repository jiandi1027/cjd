   <select id="findByParentIdsLike" resultMap="BaseResultMap" parameterType="com.rowell.sifa.pojo.sys.SysPermisson">
		SELECT
			a.id,
			a.parent_ids
		FROM SYS_PERMISSON a
		WHERE a.del_flag = #{delFlag} AND a.parent_ids LIKE #{parentIds}
		ORDER BY a.PER_SORT
	</select>
	
	<update id="updateParentIds">
