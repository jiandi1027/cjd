<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<style media="print">
.noprint {
	display: none
}

</style>
<style>
.sm{font-size:10px;}
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,select{margin:0;padding:0;}
img{border:0;}

/*================== general classes =====================*/
.clear{clear:both;}
.hide{display:none}
.left{float:left;}
.right{float:right;}
.clear{clear:both;}
.clear-l{clear:left;}
.clear-r{clear:right;}
.center{text-align:center}
.tleft{text-align:left;}
.tright{text-align:right;}
/*=================== end general classes ================*/
.content{width:690px;margin:45px auto;text-align:center;padding-bottom:20px;}
.title{font-size:28px;font-weight:bold;letter-spacing: 2px;padding:15px 0;}
.subtitle{font-size:20px;width:690px;font-weight:bold;}

.table{width:690px;border:2px solid #000;margin-top:2px;margin-top:20px;border-collapse:collapse;}
.table td{border-bottom:1px solid #000;border-right:1px solid #000;height:36px;margin:0;padding:0;}

.textp{width:690px;text-align:left;line-height:150%;}
.texts{text-decoration:underline;}
.bold{font-weight:bold;}

.pnext { page-break-after: always;} 

.nobordertable td{
	border:0 !important;
	border-bottom:0 !important;
}

</style>
<div style="margin:0 2px;padding:0;font-size:16px;font-family:Arial, Helvetica, sans-serif;">
	<div id="idControls" style="text-align:center;margin-top:10px;" class="noprint"><input disabled type="button"
		value="打印当前页" onclick="window.print()" />&nbsp;&nbsp;
	</div>
	<div class="content" >
			<div class="title" style="padding-bottom:0;margin-top:50px;">社区矫正工作季度统计表</div><br/>
			<table class="subtitle" style="" cellspacing=0 cellpadding=0 border=0>
					<tr>
						<td ><span class="texts">&nbsp;&nbsp;${culpritWorkTj.groupName}&nbsp;&nbsp;&nbsp;&nbsp;</span>县（市、区）</td>
						<td >&nbsp;&nbsp;${culpritWorkTj.year}&nbsp;&nbsp;年第&nbsp;&nbsp;${culpritWorkTj.jd}&nbsp;&nbsp;季度</td>
						<td ></td>
					</tr>
			</table>
			<br/>
			<table border=0 cellspacing=0 cellpadding=0 class="table h30"
				style="margin-top:0px;">
				<tr>
					<td rowspan="3" width="50px;">序号</td>
					<td rowspan="3" width="200px;">乡镇（街道）</td>
					<td colspan="6" align="center">司法所工作人员情况</td>
					<td rowspan="3" width="100px;">社区矫正人员总数</td>
				    <td rowspan="3" width="100px;">外出社区矫正人员数</td>
					<td rowspan="3" width="80px;">备注</td>
				</tr>
				<tr>
				    <td rowspan="2" width="90px;">工作人员总数</td>
				    <td colspan="4" align="center">编制情况</td>
					<td rowspan="2" width="90px;">司法所性质</td>
				</tr>
				<tr>
					<td width="90px;">司法行政编制人数</td>
					<td width="90px;">地方行政编制人数</td>
					<td width="90px;">事业编制人数</td>
					<td width="90px;">聘用人员人数</td>
				</tr>
 <c:set var="num" value="0"/>
<c:forEach var="item" items="${model.sqjzgzmodel}" varStatus="tag"> --%>
				<tr>
				  <td>${tag.count}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.groupName}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.sfsgzryzs}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.sfXzbz}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.dfXzbz}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.sybz}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.pyry}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.sfskind}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.allPersons}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.outPersons}&nbsp;</td>
				  <td class="sm">${culpritWorkTj.note}&nbsp;</td>
				</tr>
<c:if test="${ tag.count% 19 ==0  &&  model.sqjzgzmodel[tag.count]!=null }">
	</table>
		<div class="pnext">
					&nbsp;
		</div>
					<table border=0 cellspacing=0 cellpadding=0 class="table h30"
				style="margin-top:0px;">
				</c:if> 
		<c:set var="num" value="${num+1}" />
 </c:forEach> 
			<c:if test="${num<19}">
					<c:forEach begin="1" end="${19-num}" varStatus="tag">
				<tr>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				  <td></td>
				</tr>
			</c:forEach>
			</c:if>
			</table>
			<p class="textp">注： 1. 没有社区矫正人员的乡镇（街道）也要填入表格。<br></p>
			<p class="textp">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;2. 每季度第一个月5日前上报。
			</p>
			</div>
</div>

	