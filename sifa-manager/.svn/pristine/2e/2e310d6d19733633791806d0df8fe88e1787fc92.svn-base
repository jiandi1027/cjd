  <update id="updateByPrimaryKeySelective" parameterType="com.rowell.sifa.pojo.dailymgr.AskLeave">
    update SF_COMMEND
    <set>
      <if test="processInstanceId != null">
        PROCESS_ID = #{processInstanceId,jdbcType=VARCHAR}
      </if>
      
    </set>
    where ID = #{id,jdbcType=VARCHAR}
  </update>
  <sql id="findAllWhere">
  	 <if test="culpritName != null and culpritName!=''">
      AND t2.name LIKE CONCAT(CONCAT('%',#{culpritName}),'%')
    </if>
    <if test="groupName != null and groupName!=''">
      
       <![CDATA[  
     	 AND  exists ( select t4.id from (select * from sys_group start with id= #{groupId,jdbcType=VARCHAR} connect by prior id = parent_id) t4 where t3.ID=t4.id)
     ]]> 
    </if>
  	<if test="doDate != null and doDate !=''">
  		<![CDATA[
  			AND t.DO_DATE >= #{doDate,jdbcType=TIMESTAMP}
  		]]>
  	</if>
  	<if test="endDoDate != null and endDoDate !=''">
  		<![CDATA[
  			AND t.DO_DATE <= #{endDoDate,jdbcType=TIMESTAMP}
  		]]>
  	</if>
  	<if test="searchStartDate != null and searchStartDate !=''">
  		<![CDATA[
  			AND t.BEGIN_DATE >= #{searchStartDate,jdbcType=TIMESTAMP}
  		]]>
  	</if>
  	<if test="searchEndDate != null and searchEndDate !=''">
  		<![CDATA[
  			AND t.BEGIN_DATE <= #{searchEndDate,jdbcType=TIMESTAMP}
  		]]>
  	</if>
  	<if test="culpritId != null and culpritId!=''">
      AND t.CULPRIT_ID  = #{culpritId,jdbcType=VARCHAR}
    </if>
    <if test="reason != null and reason!=''">
      AND t.reason  = #{reason,jdbcType=VARCHAR}
    </if>
  </sql>
