  <select id="selectAll" parameterType="com.rowell.sifa.pojo.rewardspunishment.Commend" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
      from SF_COMMEND t left join SF_CULPRIT t2 on (t.CULPRIT_ID=t2.id) left join sys_group t3 on (t2.group_id=t3.id)
    <where>
      <include refid="findAllWhere" />
    </where>
