  <select id="selectAll" parameterType="com.rowell.sifa.pojo.endcorrect.Imprison" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />,
    <![CDATA[
      t2.name as culprit_name,t3.name as group_name
    from SF_IMPRISON t,SF_CULPRIT t2 , sys_group t3
      ]]>
    <where>
    	t.CULPRIT_ID=t2.id and t2.group_id=t3.id 
      <include refid="findAllWhere" />
    </where>
    <if test="orderStr!=null">
      <![CDATA[
           order by  ${orderStr}
      ]]>
    </if>
  </select>
  <select id="getCount" parameterType="com.rowell.sifa.pojo.endcorrect.Imprison" resultType="int">
   <![CDATA[
    select count(*)

    from SF_IMPRISON t where 1=1
      ]]>
    <if test="searchStartDate != null">
    <![CDATA[ 
      AND t.APPLY_DATE >= #{searchStartDate,jdbcType=TIMESTAMP}
       ]]> 
    </if>
    
    <if test="searchEndDate != null">
    	 <![CDATA[  
      AND t.APPLY_DATE <= #{searchEndDate,jdbcType=TIMESTAMP}
       ]]>  
    </if>
  </select>
  <sql id="findAllWhere">
  	<if test="culprit!=null and culprit.name != null and culprit.name!=''">
      AND t2.NAME like '%'|| #{culprit.name,jdbcType=VARCHAR}||'%'
    </if>
    <if test="groupId != null and groupId!=''">
      AND  exists ( select g.id from (select * from sys_group  start with id= #{groupId,jdbcType=VARCHAR} connect by prior id = parent_id) g left join SF_CULPRIT c on g.id=c.group_id where c.id=t.CULPRIT_ID)
    </if> 
    <if test="culpritId != null and culpritId!=''">
      AND t.CULPRIT_ID = #{culpritId,jdbcType=VARCHAR}
    </if>
    <if test="recordId != null and recordId!=''">
      AND t.RECORD_ID = #{recordId,jdbcType=VARCHAR}
    </if>
    <if test="applyDate != null">
      AND t.APPLY_DATE = #{applyDate,jdbcType=TIMESTAMP}
    </if>
    <if test="searchStartDate != null">
    <![CDATA[ 
      AND t.APPLY_DATE >= #{searchStartDate,jdbcType=TIMESTAMP}
       ]]> 
    </if>
    
    <if test="searchEndDate != null">
    	 <![CDATA[  
      AND t.APPLY_DATE <= #{searchEndDate,jdbcType=TIMESTAMP}
       ]]>  
    </if>
    <if test="reason != null and reason!=''">
      AND t.REASON = #{reason,jdbcType=CLOB}
